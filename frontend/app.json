      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",

        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.ssafyD101.walfy",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBEmWSXbZV3mg-KQ15Y2zovXv_qS87FTmE"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
      }
    }
  }
